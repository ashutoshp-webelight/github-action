{
  "name": "@tappstr/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:tappui:prod": "cd apps/tappui && npx next build",
    "build:tappuiadmin:dev": "nx run tappuiadmin:build:development",
    "build:tappuiadmin:prod": "nx run tappuiadmin:build:production",
    "prettier:fix": "prettier --write \"**/*.+(md|js|jsx|tsx|ts|json)\"",
    "lint:tappui": "nx lint tappui",
    "lint:tappuiadmin": "nx lint tappuiadmin",
    "lint:uicore": "nx lint uicore",
    "nx:lint": "nx run-many --target=lint --all",
    "start:tappuiadmin": "nx serve tappuiadmin",
    "start:tappui": "nx serve tappui",
    "start:tappui:prod": "npx next start",
    "start:all": "nx run-many --parallel --target=serve --projects=tappui,tappuiadmin",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@nx/next": "^16.5.5",
    "@swc/helpers": "~0.5.0",
    "classnames": "^2.3.2",
    "i18next": "^23.4.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-chained-backend": "^4.4.0",
    "i18next-http-backend": "^2.2.1",
    "i18next-localstorage-backend": "^4.1.1",
    "next": "13.4.1",
    "next-intl": "^2.19.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.1.2",
    "react-router-dom": "6.11.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nx/eslint-plugin": "16.5.5",
    "@nx/js": "16.5.5",
    "@nx/linter": "16.5.5",
    "@nx/react": "16.5.5",
    "@nx/vite": "16.5.5",
    "@nx/workspace": "16.5.5",
    "@nxlv/python": "16.1.2",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@types/lodash": "^4.14.197",
    "@types/node": "18.14.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "~4.0.0",
    "@vitest/coverage-c8": "~0.32.0",
    "@vitest/ui": "~0.32.0",
    "autoprefixer": "10.4.13",
    "eslint": "~8.15.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.3",
    "jsdom": "~22.1.0",
    "lint-staged": "^13.2.3",
    "nx": "16.5.5",
    "postcss": "8.4.21",
    "prettier": "^2.6.2",
    "tailwindcss": "3.2.7",
    "typescript": "~5.1.3",
    "vite": "~4.3.9",
    "vite-plugin-dts": "~2.3.0",
    "vite-tsconfig-paths": "~4.2.0",
    "vitest": "~0.32.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier:fix",
      "yarn lint:tappui",
      "yarn lint:tappuiadmin",
      "yarn lint:uicore"
    ]
  }
}
